{
  "name": "puppet-king-of-time",
  "version": "1.0.0",
  "description": "Have KING OF TIME (www.kingtime.jp) like a puppet on a string",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prettier": "prettier **/*.{md,ts,json} -w",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepare": "husky install",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yujiosaka/puppet-king-of-time.git"
  },
  "keywords": [
    "king",
    "of",
    "time",
    "puppeteer",
    "headless",
    "chrome"
  ],
  "author": "Yuji Isobe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yujiosaka/puppet-king-of-time/issues"
  },
  "homepage": "https://github.com/yujiosaka/puppet-king-of-time#readme",
  "engines": {
    "node": ">=12.0.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "puppeteer": "^20.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^10.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/npm": "^10.0.4",
    "@semantic-release/release-notes-generator": "^11.0.3",
    "@types/lodash": "^4.14.195",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "prettier": "^2.8.8",
    "semantic-release": "^21.0.5",
    "typescript": "^5.1.3"
  },
  "lint-staged": {
    "**/*.ts": [
      "eslint --fix"
    ],
    "**/*.{md,ts,json}": [
      "prettier --write"
    ]
  }
}
